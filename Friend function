---------------------------------------------------------------1st program---------------------------------------------------------------------------------------------
#include<iostream>
using  namespace std;

class A{
    private:
    int num1;
    public:
    A(){
      cout<<"Enter your Favourite number : ";
      cin>>num1; 
    }
    friend int max(A,B);
};
class B{
private:
int num2;
public:
B(){
    cout<<"Enter your Favourite number : ";
    cin>>num2;
}
friend int max(A ,B);
};
int max(A n1,B n2){
    if(n1.num1>n2.num2){
        cout<<"Number 1 is greter than Number 2"<<endl;
    }
    else{
        cout<<"Number 2 is greater than Number 1"<<endl;
    }
}
int main()
{
A obj1;
B obj2;
max(obj1,obj2);
}
--------------------------------------------------------------------------- 2nd program --------------------------------------------------------------------------------
#include<iostream>
using namespace std;
class b;
class a{
  private:
  int a1;
  public:
  a(){
     a1=10;
  }
  friend show(a,b);
};
class b{
  private: 
  int b1;
  public:
  b(){
     b1=20;
  } 
  friend show(a,b);
};
int show(a o1,b o2){
  cout<<"The value stored in class a Object member is : "<<o1.a1<<endl;
  cout<<"The value stored in class b Object member is : "<<o2.b1<<endl;
  return(o1.a1+o2.b1);
}
int main(){
  a obj1;
  b obj2; 
 cout<<show(obj1,obj2);
}
